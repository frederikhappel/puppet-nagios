# This file is managed by puppet! Do not change!
pid_file=/var/run/nrpe.pid
server_port=<%= @port %>
nrpe_user=<%= @user %>
nrpe_group=<%= @group %>
allowed_hosts=<%= ["127.0.0.1", @allowed_hosts].flatten.join(',') %>
dont_blame_nrpe=1
debug=0
command_timeout=<%= @command_timeout_in_seconds %>
include_dir=<%= @nrpeddir %>
